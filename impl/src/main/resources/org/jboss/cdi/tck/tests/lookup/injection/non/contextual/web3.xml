<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

   <display-name>Dependency Injection Test</display-name>
   <servlet>
      <servlet-name>Translator</servlet-name>
      <servlet-class>org.jboss.cdi.tck.tests.lookup.injection.non.contextual.TranslatorEndpoint</servlet-class>
      <load-on-startup>1</load-on-startup>
   </servlet>
   <servlet-mapping>
      <servlet-name>Translator</servlet-name>
      <url-pattern>/translator</url-pattern>
   </servlet-mapping>

   <servlet>
      <servlet-name>TestServlet2</servlet-name>
      <servlet-class>org.jboss.cdi.tck.tests.lookup.injection.non.contextual.TestServlet2</servlet-class>
      <load-on-startup>2</load-on-startup>
   </servlet>
   <servlet-mapping>
      <servlet-name>TestServlet2</servlet-name>
      <url-pattern>/TestServlet2</url-pattern>
   </servlet-mapping>

   <filter>
      <filter-name>TestFilter2</filter-name>
      <filter-class>org.jboss.cdi.tck.tests.lookup.injection.non.contextual.TestFilter2</filter-class>
   </filter>
   <filter-mapping>
      <filter-name>TestFilter2</filter-name>
      <url-pattern>/TestFilter2</url-pattern>
   </filter-mapping>

   <session-config>
      <session-timeout>10</session-timeout>
   </session-config>

</web-app>
